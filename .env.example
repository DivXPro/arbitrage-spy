# 日志级别配置
RUST_LOG=info

# DEX API 配置 (可选)
# UNISWAP_API_KEY=your_api_key_here
# SUSHISWAP_API_KEY=your_api_key_here
# PANCAKESWAP_API_KEY=your_api_key_here
# CURVE_API_KEY=your_api_key_here
# BALANCER_API_KEY=your_api_key_here

# 网络配置
# RPC_URL=https://mainnet.infura.io/v3/your_project_id
# CHAIN_ID=1

# WebSocket 配置 (用于实时事件监听)
# 主要的WebSocket端点，用逗号分隔多个URL
WSS_URLS=wss://mainnet.infura.io/ws/v3/your_project_id,wss://eth-mainnet.g.alchemy.com/v2/your_api_key

# 监控配置
# SCAN_INTERVAL_SECONDS=10
# MIN_PROFIT_THRESHOLD=0.01
# MAX_GAS_PRICE=50

# CoinGecko API 配置 (用于获取token数据)
# 免费版API有速率限制，建议申请API key以获得更高的请求限制
COINGECKO_API_KEY=your_coingecko_api_key_here

# TheGraph API 配置 (用于获取交易对数据)
# 免费版API有速率限制，建议申请API key以获得更高的请求限制

THEGRAPH_BASE_URL=https://gateway.thegraph.com/api
THEGRAPH_API_KEY=your_thegraph_api_key_here

UNISWAP_V2_SUBGRAPH_ID=uniswap/uniswap-v2
UNISWAP_V3_SUBGRAPH_ID=uniswap/uniswap-v3